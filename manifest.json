{
  "name": "@aduh95/dusk",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": [
        "https://outlook.live.com/*",
        "https://outlook.office365.com/*"
      ],
      "run_at": "document_end",
      "js": ["outlook.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  }
}
